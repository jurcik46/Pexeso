<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
  </startup>
  <system.serviceModel>

    <client>

      <endpoint address="http://localhost:8733/PexesoServer/ChatService"
                binding="wsDualHttpBinding"
                contract="Pexeso.Library.ServiceInterfaces.IChatService" name="WSDualHttpPexesoChatService">
      </endpoint>

      <endpoint address="net.tcp://localhost:8734/PexesoServer/ChatService"
                binding="netTcpBinding"
                contract="Pexeso.Library.ServiceInterfaces.IChatService" name="TcpPexesoChatService">
      </endpoint>


      <endpoint address="net.tcp://localhost:8734/PexesoServer/EntranceService"
                binding="netTcpBinding"
                contract="Pexeso.Library.ServiceInterfaces.IEntranceService" name="TcpPexesoEntranceService">
      </endpoint>


      <endpoint address="net.tcp://localhost:8734/PexesoServer/GameService"
                binding="netTcpBinding"
                contract="Pexeso.Library.ServiceInterfaces.IGameService" name="TcpPexesoGameService">
      </endpoint>


    </client>
  </system.serviceModel>
</configuration>